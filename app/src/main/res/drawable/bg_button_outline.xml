<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/dark_ripple_primary">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@color/dark_button_outline" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/dark_button_outline_background" />
            <corners android:radius="8dp" />
            <stroke
                android:width="1dp"
                android:color="@color/dark_button_outline" />
        </shape>
    </item>
</ripple>
